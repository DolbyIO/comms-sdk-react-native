{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@dolbyio/comms-sdk-reactnative": ["./src/index"],
      "@assets/*": ["./example/src/assets/*"],
      "@components/*": ["./example/src/components/*"],
      "@constants/*": ["./example/src/constants/*"],
      "@screens/*": ["./example/src/screens/*"],
      "@ui/*": ["./example/src/ui/*"],
      "@utils/*": ["./example/src/utils/*"]
    },
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "error",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
    // "normalTypeScriptOptions": "here"
  }
}
